{
  "name": "astro-sitio",
  "type": "module",
  "version": "0.0.1",
  "engines": {
    "node": "20.x"
  },
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "prisma generate && prisma db push && astro build",
    "build:vercel": "prisma generate && prisma db push && astro build",
    "preview": "astro preview",
    "astro": "astro",
    "postinstall": "prisma generate",
    "prisma:gen": "prisma generate",
    "prisma:migrate": "prisma migrate dev -n init",
    "prisma:deploy": "prisma db push",
    "seed": "tsx scripts/seedProducts.ts",
    "seed:products": "tsx scripts/seedProducts.ts",
    "deploy": "npm run prisma:gen && npm run prisma:deploy && npm run seed && npm run build"
  },
  "dependencies": {
    "@astrojs/react": "^3.6.0",
    "@astrojs/tailwind": "^5.1.5",
    "@astrojs/vercel": "^7.8.0",
    "@prisma/client": "^5.19.0",
    "astro": "^4.15.0",
    "nanoid": "^5.1.5",
    "react": "^18.3.0",
    "react-dom": "^18.3.0",
    "tailwindcss": "^3.4.0"
  },
  "devDependencies": {
    "@types/node": "^24.3.1",
    "@types/node-fetch": "^2.6.13",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "node-fetch": "^3.3.2",
    "prisma": "^5.19.0",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2"
  }
